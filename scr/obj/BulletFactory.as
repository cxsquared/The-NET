package obj{	import flash.display.MovieClip;	public class BulletFactory	{		public function BulletFactory()		{		}		static public function makeBullet(type:String, startX:Number, startY:Number):MovieClip		{			switch (type)			{				case "quickAttack" :					return new QuickBullet(startX, startY);					break;				case "strongAttack" :					return new StrongBullet(startX, startY);					break;				case "powerAttack" :					return new PowerBullet(startX, startY);					break;				default :					return new QuickBullet(startX, startY);			}		}	}}