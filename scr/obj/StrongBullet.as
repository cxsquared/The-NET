package obj {		import flash.display.MovieClip;	import flash.events.Event;	import flash.display.Stage;			public class StrongBullet extends MovieClip {		//Const		private const TYPE:String = "strong";				//Varibles		private var _vx:Number;		private var _vy:Number;		private var _startX:Number;		private var _startY:Number;		private var _stage:Stage;		private var _instance:StrongBullet;				public function StrongBullet(startX:Number, startY:Number) {			_startX = startX;			_startY = startY;						addEventListener(Event.ADDED_TO_STAGE, onAddedToStage);						Main.getInstance().addChild(this);		}				//When created		private function onAddedToStage(event:Event):void{			//Initialize vars			this.x = _startX;			this.y = _startY;			_vx = 20;			_vy = 0;			_instance = this;			_stage = Main.getStage();						dispatchEvent(new Event("bulletCreated", true));						//Event Handlers			addEventListener(Event.ENTER_FRAME, bulletLoop);			addEventListener(Event.REMOVED_FROM_STAGE, onRemoved);		}				//Bullet loop		private function bulletLoop(event:Event):void {			//Move bullet			x += _vx;			y += _vy;						if (this.x - this.width > _stage.stageWidth)			{				parent.removeChild(this);			}		}				public function getType():String{			return TYPE;		}				public function getInstance():StrongBullet{			return _instance;		}		//When Removed		private function onRemoved(event:Event):void {			removeEventListener(Event.ENTER_FRAME, bulletLoop);			removeEventListener(Event.ADDED_TO_STAGE, onAddedToStage);			removeEventListener(Event.REMOVED_FROM_STAGE, onRemoved);		}	}	}